{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "member-access":false,
        "class-name": true,
        "no-consecutive-blank-lines":false,
        "eofline":false,
        "component-state-and-props-no-any": true,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-trailing-whitespace": true,
        "no-unsafe-finally": true,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "semicolon": [
            true,
            "always",
            "strict-bound-class-methods"
        ],
        "quotemark": [
            true,
            "double"
        ]
    }
}